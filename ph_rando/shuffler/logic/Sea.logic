    node Treasure5: chest Treasure5
    node Treasure6: chest Treasure6
    node Treasure7: chest Treasure7

    # TO NW SEA CHART
    node ToMainNW: door MainNW
    East <-> ToMainNW: item NWSeaChart
    node ToFogNW: door FogNW
    West <-> ToFogNW: item NWSeaChart

    # TO SE SEA CHART
    node ToSE: door SE
