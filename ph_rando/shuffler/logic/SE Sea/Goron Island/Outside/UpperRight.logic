area GoronIsland:
  room UpperRight:
    node Rightmost:
      enemy BlueChu1
      enemy BlueChu2
      enemy RockChu1
      enemy RockChu2
      door ToSouth

    node Leftmost:
      door ToWestUpper
      door ToWestLowest
      set GoronMazeSwitch

    node LeftmostAfterBomb:
      door ToWestUppest

    node ChestAfterBombchu:
      chest BombchuChest

    node BeforeChestArea:
      door ToWestLower

    node ChestArea:
      chest ChestBehindSpikes

    Rightmost -> Leftmost: macro Bombs
    Leftmost -> Rightmost
    LeftmostAfterBomb <-> Leftmost: macro Bombs
    Rightmost <-> ChestAfterBombchu:  macro Bombs

    BeforeChestArea <-> ChestArea: flag GoronMazeSwitch
